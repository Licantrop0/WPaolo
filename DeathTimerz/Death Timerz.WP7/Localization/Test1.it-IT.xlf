<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="it-IT" original="Test1.resx" tool-id="MultilingualAppToolkit" product-name="DeathTimerz" product-version="2.0.0" build-num="0">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="2.1.2010.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="Resx">
        <trans-unit id="Resx/Alcohol" translate="yes" xml:space="preserve">
          <source>How often do you drink alcohol?</source>
          <target state="translated">Quante volte bevi superalcolici?</target>
        </trans-unit>
        <trans-unit id="Resx/Alcohol1" translate="yes" xml:space="preserve">
          <source>I'm teetotal</source>
          <target state="translated">Sono astemio</target>
        </trans-unit>
        <trans-unit id="Resx/Alcohol2" translate="yes" xml:space="preserve">
          <source>Without exceeding</source>
          <target state="translated">Senza eccedere</target>
        </trans-unit>
        <trans-unit id="Resx/Alcohol3" translate="yes" xml:space="preserve">
          <source>Every day</source>
          <target state="translated">Tutti i giorni</target>
        </trans-unit>
        <trans-unit id="Resx/Cigarettes1" translate="yes" xml:space="preserve">
          <source>How many cigarettes do you smoke daily?</source>
          <target state="translated">Quante sigarette fumi al giorno?</target>
        </trans-unit>
        <trans-unit id="Resx/Cigarettes2" translate="yes" xml:space="preserve">
          <source>For how many years do you smoke?</source>
          <target state="translated">Da quanti anni fumi?</target>
        </trans-unit>
        <trans-unit id="Resx/ClinicChecks" translate="yes" xml:space="preserve">
          <source>How often you make clinic check?</source>
          <target state="translated">Quante volte fai visite mediche?</target>
        </trans-unit>
        <trans-unit id="Resx/ClinicChecks1" translate="yes" xml:space="preserve">
          <source>The last one was long time ago</source>
          <target state="translated">L'ultima è stata mesi fa</target>
        </trans-unit>
        <trans-unit id="Resx/ClinicChecks2" translate="yes" xml:space="preserve">
          <source>Only when I'm sick</source>
          <target state="translated">Solo se sono malato</target>
        </trans-unit>
        <trans-unit id="Resx/ClinicChecks3" translate="yes" xml:space="preserve">
          <source>Periodically</source>
          <target state="translated">Periodicamente</target>
        </trans-unit>
        <trans-unit id="Resx/CronicDisease" translate="yes" xml:space="preserve">
          <source>Do you have chronic diseases?</source>
          <target state="translated">Soffri di malattie croniche?</target>
        </trans-unit>
        <trans-unit id="Resx/CronicDisease1" translate="yes" xml:space="preserve">
          <source>No</source>
          <target state="new">No</target>
        </trans-unit>
        <trans-unit id="Resx/CronicDisease2" translate="yes" xml:space="preserve">
          <source>Yes, mild</source>
          <target state="translated">Si, lievi</target>
        </trans-unit>
        <trans-unit id="Resx/CronicDisease3" translate="yes" xml:space="preserve">
          <source>Yes, serious</source>
          <target state="translated">Si, gravi</target>
        </trans-unit>
        <trans-unit id="Resx/Drugs" translate="yes" xml:space="preserve">
          <source>Do you use drugs?</source>
          <target state="translated">Ti droghi?</target>
        </trans-unit>
        <trans-unit id="Resx/Drugs1" translate="yes" xml:space="preserve">
          <source>Never taked</source>
          <target state="translated">No, mai fatto</target>
        </trans-unit>
        <trans-unit id="Resx/Drugs2" translate="yes" xml:space="preserve">
          <source>Sometimes</source>
          <target state="translated">Ogni tanto</target>
        </trans-unit>
        <trans-unit id="Resx/Drugs3" translate="yes" xml:space="preserve">
          <source>Usually</source>
          <target state="translated">Spesso</target>
        </trans-unit>
        <trans-unit id="Resx/Food" translate="yes" xml:space="preserve">
          <source>What kind of diet do you follow?</source>
          <target state="translated">Che tipo di dieta segui?</target>
        </trans-unit>
        <trans-unit id="Resx/Food1" translate="yes" xml:space="preserve">
          <source>Mediterranean</source>
          <target state="translated">Mediterranea</target>
        </trans-unit>
        <trans-unit id="Resx/Food2" translate="yes" xml:space="preserve">
          <source>I eat everything</source>
          <target state="translated">Mangio di tutto</target>
        </trans-unit>
        <trans-unit id="Resx/Food3" translate="yes" xml:space="preserve">
          <source>Fat is good</source>
          <target state="translated">Grasso è bello</target>
        </trans-unit>
        <trans-unit id="Resx/Food4" translate="yes" xml:space="preserve">
          <source>Vegetarian</source>
          <target state="translated">Vegetariana</target>
        </trans-unit>
        <trans-unit id="Resx/Height" translate="yes" xml:space="preserve">
          <source>Insert your height (in)</source>
          <target state="translated">Inserisci la tua altezza (cm)</target>
        </trans-unit>
        <trans-unit id="Resx/Sports" translate="yes" xml:space="preserve">
          <source>Do you do some sport?</source>
          <target state="translated">Pratichi qualche sport?</target>
        </trans-unit>
        <trans-unit id="Resx/Sports1" translate="yes" xml:space="preserve">
          <source>Not so much / never</source>
          <target state="translated">Poco / per niente</target>
        </trans-unit>
        <trans-unit id="Resx/Sports2" translate="yes" xml:space="preserve">
          <source>Weekly</source>
          <target state="translated">Settimanalmente</target>
        </trans-unit>
        <trans-unit id="Resx/Sports3" translate="yes" xml:space="preserve">
          <source>Daily</source>
          <target state="translated">Tutti i giorni</target>
        </trans-unit>
        <trans-unit id="Resx/Weight" translate="yes" xml:space="preserve">
          <source>Insert your weight (lb)</source>
          <target state="translated">Inserisci il tuo peso (kg)</target>
        </trans-unit>
      </group>
    </body>
  </file>
